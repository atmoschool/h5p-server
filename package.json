        "ajv-keywords": "^4.0.0",
        "axios": "^0.21.0",
        "cache-manager": "^3.3.0",
        "crc": "^3.8.0",
        "debug": "^4.1.1",
        "fs-extra": "^9.0.0",
        "glob-promise": "^3.4.0",
        "image-size": "^0.9.0",
        "jsonpath": "^1.0.2",
        "merge": "^2.0.0",
        "mime-types": "^2.1.26",
        "nanoid": "^3.1.10",
        "promisepipe": "^3.0.0",
